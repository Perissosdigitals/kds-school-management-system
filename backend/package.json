{
  "name": "@kds/backend",
  "version": "1.0.0",
  "description": "KDS School Management System - Backend API (NestJS Monorepo)",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev:gateway",
    "dev:gateway": "cd apps/api-gateway && npm run start:dev",
    "dev:worker": "cd apps/queue-worker && npm run start:dev",
    "dev:realtime": "cd apps/realtime && npm run start:dev",
    "build": "npm run build --workspaces",
    "build:gateway": "cd apps/api-gateway && npm run build",
    "start": "npm run start:gateway",
    "start:gateway": "cd apps/api-gateway && npm run start:prod",
    "start:worker": "cd apps/queue-worker && npm run start:prod",
    "lint": "eslint \"{apps,packages}/**/*.ts\"",
    "format": "prettier --write \"{apps,packages}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migration:generate": "typeorm migration:generate -d shared/database/data-source.ts",
    "migration:run": "typeorm migration:run -d shared/database/data-source.ts",
    "migration:revert": "typeorm migration:revert -d shared/database/data-source.ts",
    "seed": "cd apps/api-gateway && ts-node -r tsconfig-paths/register src/database/seeds/seed.ts",
    "db:seed": "npm run seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "aws-sdk": "^2.1532.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hono": "^4.10.6",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "socket.io": "^4.6.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/papaparse": "^5.3.14",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
